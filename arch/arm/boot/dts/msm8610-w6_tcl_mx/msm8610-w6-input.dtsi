&soc {
	i2c@f9923000{
		melfas_mms136@48 {
			compatible = "melfas,mms136";
			reg = <0x48>;
			interrupt-parent = <&msmgpio>;
			interrupts = <0 0x0>;
			vdd-supply = <&pm8110_l19>;
			vcc_i2c-supply = <&pm8110_l14>;
			melfas,i2c_int_gpio = <&msmgpio 0 0x00>;
			melfas,i2c_sda_gpio = <2>;
			melfas,i2c_scl_gpio = <3>;
			melfas,i2c-pull-up = <1>;
			melfas,auto_fw_update = <1>;
			melfas,enalbe_active_mode = <0>;
			melfas,product = "S4A50A";	
			melfas,panel-x = <480>;
			melfas,panel-y = <800>;
			melfas,fw-image-name = "melfas/w6_tcl_mx/w6_tcl_mx_v003.mfsb";
			melfas,button-map = <158 139>;
			melfas,use_vdd_i2c = <0>;
			melfas,use_vdd = <0>;
			melfas,gpio-vdd-en = <82>;
			status = "disable";
		};
		
		lge_touch@48 {
			revision = "rev_a";
			status = "ok";
			lge,ic_type = <1>;
			lge,product = "S4A50A";
			lge,fw_image = "melfas/w6_tcl_mx/w6_tcl_mx_v003.mfsb";
			button_name = <158 139>;
			x_max = <480>;
			y_max = <800>;
			lcd_x = <480>;
			lcd_y = <800>;
			vdd_type0 = <1>;
			vdd_name0 = "high";
			vdd_value0 = <75>;
			vdd_type1 = <2>;
		};
		
		synaptics_red@20 {
			status = "disable";
			revision = "rev_b...";
			synaptics,button-map = <158 139>;
			synaptics,i2c-pull-up = <0>;
			synaptics,power-down  = <1>;
			synaptics,gpio_vdd_en  = <75>;
		};

		synaptics_s220x@20 {
			status = "ok";
			revision = "rev_b...";
			synaptics,fw_version_info = <0x12 0x05 0x01>;
			synaptics,fw_image = "synaptics/w6_tcl_mx/PLG337-T001-PR1592892-DS4.3.5.1.16_12050101.img";
			synaptics,panel_spec = "synaptics/w6_tcl_mx/w6_tcl_mx_limit.txt";
                        synaptics,global_access_pixel = <10>;
			lge,knock_on_type = <1>;
			synaptics,platform_data	{
				number_of_button = <2>;
				button_name = [9e 8b];
				x_max = <960>;
				y_max = <1600>;
				lcd_x = <480>;
				lcd_y = <800>;
				use_vio_regulator = <1>;
				gpio_vdd_en = <75>;
			    /* firmware image depends on this value, temporarily used */
			    /* 0: No image(default), 1: W3DS(TIM BR)(PLG312_T001), 2: W3S(VDF)(PLG311_T001), 3:X3, 4:W5DS, 5:W5S ... */
				panel_type = <4>;
				/*maker_id  disable(0), enable(1)*/
				maker_id = <1>;
				maker_id_gpio = <76>;
                                palm_detect_mode = <1>;
                                ghost_detection_enable = <0>;
			};		
		};
	};
};
