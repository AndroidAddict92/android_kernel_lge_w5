&soc {
	i2c@f9923000{
		mms100s@48 {
			melfas,product = "T4S35A";
			melfas,use_vdd = <0>;
			melfas,use_vdd_int = <0>;
			melfas,use_vdd_i2c = <1>;
			melfas,gpio-vdd-en = <62>;
			melfas,key-map = <158 172 139 249>;
			melfas,fw-image-name = "melfas/w3ds_open_br/w3ds_open_br_rev_a_v103.mfsb";
			melfas,panel-spec-name = "melfas/w3ds_open_br/w3ds_open_br_limit.txt";
			status = "ok";
			revision = "rev_a...";
		};

		imagis_ist30xx@50 {
			compatible = "imagis,ist30xx";
			reg = <0x50>;
			interrupt-parent = <&msmgpio>;
			interrupts = <0 0x2>;
			vdd-supply = <&pm8110_l22>;
			vcc_i2c-supply = <&pm8110_l14>;
			imagis,i2c_int_gpio = <&msmgpio 0 0x00>;
			imagis,panel-x = <320>;
			imagis,panel-y = <480>;
			imagis,i2c-pull-up = <1>;
			status = "ok";
			revision = "rev_0";
		};

		lge_touch@48 {
			revision = "rev_a...";
			status = "disable";
			lge,product = "T4S35A";
			lge,fw_image = "melfas/w3ds_open_br/w3ds_open_br_rev_a_v103.mfsb";
			lge,panel_spec = "melfas/w3ds_open_br/w3ds_open_br_limit.txt";
			vdd_type0 = <1>;
			vdd_name0 = "high";
			vdd_value0 = <62>;
			vdd_type1 = <2>;
		};
	};
};
